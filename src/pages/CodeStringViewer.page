<apex:page standardController="Asset" extensions="CodeStringViewerController">

<apex:PageBlock >
   <apex:PageBlockTable var="c" value="{!CodeStringRecs}" title="Main Item">
   
   <apex:column headerValue="Sequence" value="{!c.code_sequence}" />
   <apex:column headerValue="Value" value="{!c.code_value}" />
   <apex:column headerValue="Description" value="{!c.code_description}" />
     
   </apex:PageBlockTable>
</apex:PageBlock>

</apex:page>