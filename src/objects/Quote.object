        <fullName>LATAM_Quote_Status_Submit_for_Approvals</fullName>
        <active>true</active>
        <errorConditionFormula>!IsSyncing &amp;&amp;  ISPICKVAL( Quote_Status__c , &quot;Submit for Approvals&quot;)</errorConditionFormula>
