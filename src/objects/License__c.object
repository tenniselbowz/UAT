    <validationRules>
        <fullName>Coverage_Class_equals_Other</fullName>
        <active>true</active>
        <description>If the Coverage Class = Other then the user must enter something into the Other Coverage Class field.</description>
        <errorConditionFormula>(ISBLANK(Other_Coverage__c ) &amp;&amp; Text(License_Coverage__c)= &quot;Other&quot;)</errorConditionFormula>
        <errorDisplayField>Other_Coverage__c</errorDisplayField>
        <errorMessage>If License Coverage = Other the Other Coverage field must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_is_Suspended_or_Cancelled</fullName>
        <active>true</active>
        <description>If Status = Suspended or Cancelled the Status Notes cannot be blank</description>
        <errorConditionFormula>AND(
OR (Text( Status__c)= &quot;Suspended&quot;, Text( Status__c) =&quot;Cancelled&quot;),
ISBLANK( Status_Note__c))</errorConditionFormula>
        <errorDisplayField>Status_Note__c</errorDisplayField>
        <errorMessage>If the Status is Suspended or Cancelled you must add a Status Note.</errorMessage>
    </validationRules>
